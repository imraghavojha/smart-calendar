{
    "model": {
        "name": "t5-large",
        "max_length": 256,
        "batch_size": 2,
        "learning_rate": 2e-4,
        "epochs": 3,
        "gradient_accumulation_steps": 4,
        "warmup_steps": 100,
        "weight_decay": 0.01,
        "train_display_steps": 10
    },
    "data": {
        "train_examples": 90,
        "validation_split": 0.1,
        "max_examples_per_type": 50,
        "min_examples_per_type": 10
    },
    "output": {
        "model_dir": "models",
        "checkpoint_dir": "checkpoints",
        "log_dir": "logs"
    },
    "optimization": {
        "quantize": true,
        "quantization_type": "dynamic",
        "target_size": "int8",
        "pruning": false
    }
}